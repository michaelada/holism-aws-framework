{
  "name": "@aws-web-framework/backend",
  "version": "1.0.0",
  "description": "Backend service for AWS Web Application Framework",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "migrate:up": "node-pg-migrate up -m migrations",
    "migrate:down": "node-pg-migrate down -m migrations",
    "migrate:create": "node-pg-migrate create -m migrations"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.515.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.2",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "prom-client": "^15.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/node-pg-migrate": "^2.3.1",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "fast-check": "^3.15.1",
    "jest": "^29.7.0",
    "node-pg-migrate": "^8.0.4",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1"
  }
}
